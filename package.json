    "name": "Documentação - Node Task API",
    "template": {
      "forceLanguage": "pt_br"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeandobre/api-rest-node.git"
  },
  "keywords": [
    "api",
    "rest"
  ],
  "author": "Jean Alexandre Dobre <jeandobre@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeandobre/api-rest-node/issues"
  },
  "homepage": "https://github.com/jeandobre/api-rest-node#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "consign": "^0.1.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jwt-simple": "^0.5.6",
    "morgan": "^1.9.1",
    "npm": "^6.13.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "sequelize": "^5.21.4",
